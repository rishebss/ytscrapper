{
    "builds": [
        {
         "src": "customproject/wsgi.py",
          "use": "@vercel/python",
          "config": {
            "maxLambdaSize": "300mb",
            "runtime": "python3.12"
          }
        }
    ],
    "routes": [
      {
        "src": "/(.*)",
         "dest": "customproject/wsgi.py"
      }
    ]
  }